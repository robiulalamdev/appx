{
  "expo": {
    "name": "drivereta",
    "slug": "drivereta",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "drivereta",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/adaptive-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F1F5F9"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#F1F5F9"
      },
      "permissions": [
        "INTERNET"
      ],
      "package": "com.robiulalamdx.drivereta"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/lato/Lato-Regular.ttf",
            "./assets/fonts/lato/Lato-SemiBold.ttf",
            "./assets/fonts/lato/Lato-Bold.ttf",
            "./assets/fonts/lato/Lato-Medium.ttf",
            "./assets/fonts/archivo/Archivo-Regular.ttf",
            "./assets/fonts/inter/Inter-Regular.otf",
            "./assets/fonts/inter/Inter-Medium.otf",
            "./assets/fonts/inter/Inter-SemiBold.otf",
            "./assets/fonts/inter/Inter-Bold.otf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a9ba4541-bdca-431e-9357-374bd2baeab5"
      }
    }
  }
}